{
  "ci": {
    "collect": {
      "startServerCommand": "npm run lhci:server",
      "startServerReadyPattern": "Local:",
      "url": [
        "http://localhost:4200/",
        "http://localhost:4200/accessibility-today",
        "http://localhost:4200/accessibility", 
        "http://localhost:4200/universal",
        "http://localhost:4200/healthcare",
        "http://localhost:4200/rights",
        "http://localhost:4200/inclusivity",
        "http://localhost:4200/phisical",
        "http://localhost:4200/a11y"
      ],
      "numberOfRuns": 2
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["warn", {"minScore": 0.8}],
        "categories:seo": ["warn", {"minScore": 0.7}],
        "categories:performance": ["warn", {"minScore": 0.6}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}